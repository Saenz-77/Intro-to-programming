class Main {
    public static void main(String[] args) {
        int result;
        result = suma(1, 2, 3);
        System.out.println(result);
        
        
        Main miCoche = new Main();
        
    }
    public static int suma(int num1, int num2, int num3){
        return num1 + num2 + num3;
    }
}

class coche {
    int puertas;
    public int masPuertas(int add){
        return puertas + add;
    }
    
}
